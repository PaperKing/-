icon/home.png'),\n          selectedIcon:require('../../assets/icon/home_selected.png'),\n          tabName:'首页',\n          selected:true,\n          target:'Main'\n        },\n        {\n          icon:require('../../assets/icon/course.png'),\n          selectedIcon:require('../../assets/icon/course_selected.png'),\n          tabName:'课程',\n          selected: false,\n          target: 'Course'\n        },\n        {\n          icon:require('../../assets/icon/doWork.png'),\n          selectedIcon:require('../../assets/icon/doWork_selected.png'),\n          tabName:'考试',\n          selected: false,\n          target: 'Exam'\n        },\n        {\n          icon:require('../../assets/icon/questions.png'),\n          selectedIcon:require('../../assets/icon/questions_selected.png'),\n          tabName:'题库',\n          selected: false,\n          target: 'Questions'\n        },\n      ]\n    }\n  },\n  methods:{\n    selectTab(index){\n      for (let i = 0; i <this.tabs.length ; i++) {\n        this.tabs[i].selected = i === index;\n      }\n      this.$router.push({name:this.tabs[index].target});\n    }\n  }\n}\n</script>\n\n<style scoped>\n.container{\n  width: 100%;\n  height: 100%;\n  /*background-color: lightskyblue;*/\n}\n.logoContainer{\n  width: 100%;\n  height: 16%;\n  display: flex;\n  justify-content: center;\n  align-items: center;\n  /*background-color: lightyellow;*/\n}\n.tabContainer{\n  width: 100%;\n  height: 84%;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n}\n.tabBox{\n  width: 75%;\n  height: 8%;\n  margin-top: 5%;\n  cursor: pointer;\n}\n</style>\n"]}]}